generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Data {
  id String @id @default(auto()) @map("_id") @db.ObjectId 
  title       String
  description String?
  completed   Boolean  @default(false) 
  createdAt   DateTime @default(now()) 
  updatedAt   DateTime @updatedAt 
}
